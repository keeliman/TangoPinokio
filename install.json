{
  "run": [{
    "method": "shell.run",
    "params": {
      "message": "git clone https://github.com/CyberAgentAILab/TANGO.git ."
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "python -m venv venv"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": {
        "windows": ".\\venv\\Scripts\\pip install -r requirements.txt",
        "linux": "./venv/bin/pip install -r requirements.txt",
        "darwin": "./venv/bin/pip install -r requirements.txt"
      }
    }
  }, {
    "method": "notify",
    "params": {
      "html": "TANGO installation completed! Please configure your OpenAI API key in config.py before running."
    }
  }]
} 